
{% extends 'base.html.twig' %}

{% block title %}Edytuj studenta{% endblock %}

{% block body %}
<div class="container">
    <ol class="breadcrumb p-0">
        <li class="breadcrumb-item"><a href="#">Strona główna</a></li>
        <li class="breadcrumb-item"><a href="#">{{ subjectName.content }}</a></li>
        <li class="breadcrumb-item"><a href="#">{{ subjectGroup.name }}</a></li>
        <li class="breadcrumb-item "><a href="#">{{ student.forename }} {{ student.surname }}</a></li>
        <li class="breadcrumb-item active"><a href="#">Edycja</a></li>
    </ol>

    <h1 class="mt-5 mb-3">{{ student.forename }} {{ student.surname }}</h1>
    {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
        {{ form_widget(form) }}
    {{ form_end(form) }}
</div>
{% endblock %}